  },
  "scripts": {
    "build": "npm run clean && npm run lint && webpack",
    "build:i18n": "rimraf _translations && BABEL_ENV=i18n babel --quiet src > /dev/null",
    "build:i18n:langs": "npm run build:i18n && babel-node scripts/i18nToXliff.js",
    "clean": "rimraf dist",
    "deploy": "npm run dist && gh-pages -d dist",
    "dist": "npm run clean && npm run lint && NODE_ENV=production webpack --config webpack.config.production.js",
    "lint": "eslint --ext .js --ext .jsx --fix src",
    "start": "npm run build && node server.js",
    "dev": "npm run build && DEVTOOLS=true node server.js",
    "debug": "npm run clean && webpack && DEVTOOLS=true node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint"
  ],
